block content
    if data.user.registered
        if data.user.address == current_user.address
            h2 Your Meta Mask Wallet 
                span (#{data.user.name})
                h3
        else
            h2 Profile: #{data.user.name}
        ul
            li Wallet: #{data.user.address}
            li Tags: #{data.user.tag}
            li Gold: #{data.user.gold}
            
        ul Stamper: 
            if data.stamper.exists
                li Active: 
                    span= data.stamper.active
                li Stamp Rate: 
                    span= data.stamper.stamprate
                li Min Payment: 
                    span= data.stamper.minpt
                li Last Stamp: 
                    span= data.stamper.laststamp
            else
                span No
        include carbon.pug
    else
        p Wallet not registered